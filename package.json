  "scripts": {
    "build": "ng build retail-onboarding --configuration production && ng build retail-onboarding --configuration production-co-applicant && ng build retail-onboarding-case-manager --configuration production",
    "postbuild": "cp ./docker/Dockerfile ./dist && cp ./docker/default.conf.template ./dist/retail-onboarding && cp ./docker/default.conf.template ./dist/retail-onboarding-welcome && cp ./docker/default.conf.template ./dist/retail-onboarding-case-manager",
    "serve:prospect": "ng serve retail-onboarding --port 4201 -c development",
    "serve:prospect:ru": "ng serve retail-onboarding --port 4201 -c ru",
    "serve:co-applicant": "ng serve retail-onboarding --port 4203 -c co-applicant",
    "serve:case-manager": "ng serve retail-onboarding-case-manager --port 4202",
    "format": "prettier --write \"{apps,libs}/**/*.ts\"",
    "format:check": "prettier --list-different \"{apps,libs}/**/*.ts\"",
    "lint": "ng lint",
    "test": "ng test",
    "ng": "ng",
    "i18n:extract": "ng extract-i18n --output-path locales"
  },
